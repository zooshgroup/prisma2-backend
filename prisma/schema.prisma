generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://prismatest:prismatest@localhost:5432/prismatest?schema=public"
}

model movie {
  id     Int    @id
  length Int
  title  String
}

model user {
  age      Int?
  id       String @id
  name     String
  password String
}
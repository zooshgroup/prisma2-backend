generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://prismatest:prismatest@localhost:5432/prismatest?schema=public"
}

model movie {
  id     String @id @default(uuid())
  length Int?
  title  String
}

model user {
  age      Int?
  email    String @unique
  id       String @id @default(uuid())
  name     String
  password String
}